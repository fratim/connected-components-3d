                    "outputs" : [OUTPUT_DIR+"mf-S1-out-"+BZ+"z.txt"],
                } for BZ in Z_RANGE,

                {
                    "command" : "python stepTwoA.py "+BZ+" "+OUTPUT_DIR+"mf-S1-out-"+BZ+"z.txt > "+OUTPUT_DIR+"mf-S2A-out-"+BZ+"z.txt",
