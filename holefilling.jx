                    "outputs" : [OUTPUT_DIR+"mf-S2B-out.txt"],
                },

                {
                    "command" : "python stepThree.py "+BZ+" "+OUTPUT_DIR+"mf-S2B-out.txt > "+OUTPUT_DIR+"mf-S3-out-"+BZ+"z.txt",
                    "inputs"  : [OUTPUT_DIR+"mf-S2B-out.txt"],
                    "outputs" : [OUTPUT_DIR+"mf-S3-out-"+BZ+"z.txt"],
                } for BZ in Z_RANGE,

              ]
}
