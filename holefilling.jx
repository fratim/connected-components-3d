{
    "define" : {
        "Z_RANGE"    : range(0,8),
        "OUTPUT_DIR" : "/n/pfister_lab2/Lab/tfranzmeyer/Data/1024x1024x128_mflow/Code/connected-components-3d/mfoutput/",
    },

    "rules" : [
                {
                    "command" : "python stepOne.py "+BZ+" > "+OUTPUT_DIR+"mf-S1-out-"+BZ+"z.txt",
